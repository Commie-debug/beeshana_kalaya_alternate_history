title: Presidential Elections
new-page: true
tags: event
priority: -1
view-if: (year = next_pres_election_year and month >= next_pres_election_month) or (year > next_pres_election_year)
on-departure: month_actions += 1; in_election = 0; next_pres_election_year = next_pres_election_year + 6; n_pres_elections += 1;
set-jump: post_pres_election
face-image: img/Election_Time.png

= Presidential Election, [+ month : month +] [+ year +]

The Presidential Election of Sri Lanka is to be held now. The President is elected directly by the people through a preferential voting system. Unlike parliamentary elections, presidential elections are a direct contest between candidates representing parties or coalitions.

- @pres_election_algorithm: May the best candidate win...


@pres_election_algorithm
on-arrival: {!

//ethnic groups and classes arrays
var ethnic_groups = ['sinhala', 'sltamil', 'itamil', 'muslim', 'other'];
var class_groups = ['upper', 'middle', 'worker', 'rural'];

//weight factors
var ethnic_weight = Q.ethnic_weight;
var class_weight = Q.class_weight;

//class variable mapping (to match parliament election)
var class_var_map = {
    'upper': 'upper',
    'middle': 'middle', 
    'worker': 'workers',
    'rural': 'rural'
};

//normalization of ethnic and class
for (var ethnic of ethnic_groups) {
    var ethnic_total = 0;
    for (var party of Q.parties) {
        var support = Q[ethnic + '_' + party] || 0;
        if (support < 0) support = 0;
        ethnic_total += support;
    }
    for (var party of Q.parties) {
        var support = Q[ethnic + '_' + party] || 0;
        if (support < 0) support = 0;
        Q[ethnic + '_' + party + '_norm'] = (ethnic_total > 0) ? (support / ethnic_total) : 0;
    }
}

for (var cls of class_groups) {
    var class_total = 0;
    var cls_var = class_var_map[cls];
    for (var party of Q.parties) {
        var support = Q[cls_var + '_' + party] || 0;
        if (support < 0) support = 0;
        class_total += support;
    }
    for (var party of Q.parties) {
        var support = Q[cls_var + '_' + party] || 0;
        if (support < 0) support = 0;
        Q[cls + '_' + party + '_norm'] = (class_total > 0) ? (support / class_total) : 0;
    }
}

var candidateData = {};
var processedParties = {};

//all party names including formable parties
var all_party_names = ['slfp', 'unp', 'jvp', 'cpsl', 'lssp', 'nssp', 'tulf', 'actc', 'cwc', 
                       'slmc', 'slmp', 'slpp', 'mep', 'npp', 'eros', 'dplf', 'pflt', 'ucpf'];


//endorsing parties
var endorsementMap = {}; 

for (var party of all_party_names) {
    var endorsement = Q[party + '_pres_endorsement'];
    if (endorsement && endorsement !== '' && endorsement !== party) {
        endorsementMap[party] = endorsement.toLowerCase();
        processedParties[party] = true;
    }
}

//coalition parties
var partyToCoalition = {};
var coalitionLeader = {};
var coalitionMembers = {};


for (var party of all_party_names) {
    partyToCoalition[party] = -1;
}

//mapping parties to coalitions from active_coalitions
if (Q.active_coalitions && Q.active_coalitions.length > 0) {
    for (var c = 0; c < Q.active_coalitions.length; c++) {
        var coalition = Q.active_coalitions[c];
        coalitionMembers[c] = [];
        coalitionLeader[c] = coalition.lead.toLowerCase();
        
        for (var m = 0; m < coalition.members.length; m++) {
            var memberParty = coalition.members[m].toLowerCase();
            if (!processedParties[memberParty]) {
                partyToCoalition[memberParty] = c;
                coalitionMembers[c].push(memberParty);
            }
        }
    }
}

//also check individual coalition_id variables as a backup, probably not needed nuwaraeliya_d_pres_actc
for (var pname of all_party_names) {
    var cid = Q[pname + '_coalition_id'];
    if (cid !== undefined && cid >= 0 && !processedParties[pname]) {
        partyToCoalition[pname] = cid;
        if (!coalitionMembers[cid]) {
            coalitionMembers[cid] = [];
        }
        if (coalitionMembers[cid].indexOf(pname) === -1) {
            coalitionMembers[cid].push(pname);
        }
        // Find coalition leader
        if (Q.active_coalitions && Q.active_coalitions[cid]) {
            coalitionLeader[cid] = Q.active_coalitions[cid].lead.toLowerCase();
        }
    }
}

//final candidates list
//coalition leader candidates
var processedCoalitions = {};
for (var party of all_party_names) {
    if (processedParties[party]) continue;
    
    var cid = partyToCoalition[party];
    if (cid >= 0 && !processedCoalitions[cid]) {
        var leader = coalitionLeader[cid];
        if (leader) {
            var candidateName = Q[leader + '_pres_candidate'] || (leader.toUpperCase() + ' Candidate');
            candidateData[leader] = {
                candidate: candidateName,
                baseParty: leader,
                parties: coalitionMembers[cid].slice(), // Copy array
                type: 'coalition_leader',
                coalitionId: cid,
                coalitionName: Q.active_coalitions && Q.active_coalitions[cid] ? Q.active_coalitions[cid].name : 'Coalition',
                endorsedBy: []
            };
            for (var member of coalitionMembers[cid]) {
                processedParties[member] = true;
            }
            processedCoalitions[cid] = true;
        }
    }
}

//independent parties
for (var party of all_party_names) {
    if (processedParties[party]) continue;
    
    var isActive = Q.parties.indexOf(party) !== -1;
    var isFormed = Q[party + '_formed'];
    
    if (!isActive && !isFormed) continue;
    
    var cid = partyToCoalition[party];
    if (cid < 0) {
        var candidateName = Q[party + '_pres_candidate'] || (party.toUpperCase() + ' Candidate');
        candidateData[party] = {
            candidate: candidateName,
            baseParty: party,
            parties: [party],
            type: 'independent',
            endorsedBy: []
        };
        processedParties[party] = true;
    }
}

//add endorsment votes
for (var endorsingParty in endorsementMap) {
    var endorsedParty = endorsementMap[endorsingParty];
    
    var targetCandidate = null;
    
    if (candidateData[endorsedParty]) {
        targetCandidate = endorsedParty;
    } else {
        var endorsedCid = partyToCoalition[endorsedParty];
        if (endorsedCid >= 0 && coalitionLeader[endorsedCid]) {
            targetCandidate = coalitionLeader[endorsedCid];
        }
    }
    
    if (targetCandidate && candidateData[targetCandidate]) {
        candidateData[targetCandidate].parties.push(endorsingParty);
        candidateData[targetCandidate].endorsedBy.push(endorsingParty);
    }
}

//district list (same as parliament)
var district_names = [
    'colombo_d', 'gampaha_d', 'kalutara_d',
    'jaffna_d', 'kilinochchi_d', 'mullaitivu_d', 'mannar_d', 'vavuniya_d',
    'trincomalee_d', 'batticaloa_d', 'ampara_d',
    'kandy_d', 'matale_d', 'nuwaraeliya_d',
    'galle_d', 'matara_d', 'hambantota_d',
    'kurunegala_d', 'puttalam_d',
    'anuradhapura_d', 'polonnaruwa_d',
    'badulla_d', 'monaragala_d',
    'ratnapura_d', 'kegalle_d'
];

//calculate raw party support 
var partyVotesTotal = {};
for (var party of all_party_names) {
    partyVotesTotal[party] = 0;
}

var districtPartyVotes = {};

for (var i = 0; i < district_names.length; i++) {
    var district_name = district_names[i];
    
    // Get district ethnic composition
    var district_ethnic = {
        'sinhala': Q[district_name + '_sinhala'] || 0,
        'sltamil': Q[district_name + '_sltamil'] || 0,
        'itamil': Q[district_name + '_itamil'] || 0,
        'muslim': Q[district_name + '_muslim'] || 0,
        'other': Q[district_name + '_other'] || 0
    };
    
    var district_class = {
        'upper': Q[district_name + '_upper'] || 0,
        'middle': Q[district_name + '_middle'] || 0,
        'worker': Q[district_name + '_worker'] || 0,
        'rural': Q[district_name + '_rural'] || 0
    };
    
    var district_control = Q[district_name + '_control'];
    if (district_control === undefined) district_control = 100;
    
    //pop weight
    var district_pop_weight = Q[district_name + '_pop_weight'] || 0;
    
    districtPartyVotes[district_name] = {};
    var district_total_support = 0;
    
    for (var party of all_party_names) {
        var isActive = Q.parties.indexOf(party) !== -1;
        var isFormed = Q[party + '_formed'];
        if (!isActive && !isFormed) {
            districtPartyVotes[district_name][party] = 0;
            continue;
        }
        
        //ethnic support
        var ethnic_support = 0;
        for (var ethnic of ethnic_groups) {
            ethnic_support += (district_ethnic[ethnic] / 100) * (Q[ethnic + '_' + party + '_norm'] || 0);
        }
        
        //class support
        var class_support = 0;
        for (var cls of class_groups) {
            class_support += (district_class[cls] / 100) * (Q[cls + '_' + party + '_norm'] || 0);
        }
        
        //combined support with weights
        var combined_support = (ethnic_weight * ethnic_support) + (class_weight * class_support);
        
        //reduced turnout depending on districts government control
        combined_support *= (district_control / 100);
        
        districtPartyVotes[district_name][party] = combined_support;
        district_total_support += combined_support;
        
        //add to total with population weight
        partyVotesTotal[party] += combined_support * district_pop_weight;
    }
    
    //normalize district votes to percentages
    for (var party of all_party_names) {
        if (district_total_support > 0) {
            districtPartyVotes[district_name][party] = (districtPartyVotes[district_name][party] / district_total_support) * 100;
        } else {
            districtPartyVotes[district_name][party] = 0;
        }
    }
}


var candidateVotes = {};
var totalVotes = 0;

for (var baseParty in candidateData) {
    var data = candidateData[baseParty];
    var votes = 0;
    
    // Sum votes from all contributing parties
    for (var contributing of data.parties) {
        votes += partyVotesTotal[contributing] || 0;
    }
    
    candidateVotes[baseParty] = votes;
    totalVotes += votes;
}

//combine votes towards the coalition head in districts
for (var i = 0; i < district_names.length; i++) {
    var district_name = district_names[i];
    
    for (var baseParty in candidateData) {
        var data = candidateData[baseParty];
        var district_candidate_votes = 0;
        
        for (var contributing of data.parties) {
            district_candidate_votes += districtPartyVotes[district_name][contributing] || 0;
        }
        
        Q[district_name + '_pres_' + baseParty] = Math.round(district_candidate_votes * 10) / 10;
    }
}


//winner determination
Q.pres_candidates = [];
Q.pres_results = [];

var maxVotes = 0;
var winnerParty = null;

for (var baseParty in candidateData) {
    var data = candidateData[baseParty];
    var votes = candidateVotes[baseParty];
    var percentage = (totalVotes > 0) ? (votes / totalVotes) * 100 : 0;
    percentage = Math.round(percentage * 10) / 10; // Round to 1 decimal
    
    Q[baseParty + '_pres_votes'] = percentage;
    
    Q.pres_results.push({
        party: baseParty,
        candidate: data.candidate,
        votes: percentage,
        type: data.type,
        coalitionName: data.coalitionName || null,
        endorsedBy: data.endorsedBy,
        contributingParties: data.parties
    });
    
    Q.pres_candidates.push(baseParty);
    
    if (votes > maxVotes) {
        maxVotes = votes;
        winnerParty = baseParty;
    }
}

//sorts the results by votes (descending)
Q.pres_results.sort(function(a, b) {
    return b.votes - a.votes;
});

//stores the winner's information
if (winnerParty && candidateData[winnerParty]) {
    Q.pres_winner_party = winnerParty;
    Q.pres_winner_candidate = candidateData[winnerParty].candidate;
    Q.pres_winner_votes = Q[winnerParty + '_pres_votes'];
} else {
    Q.pres_winner_party = 'none';
    Q.pres_winner_candidate = 'No Winner';
    Q.pres_winner_votes = 0;
}

//determine the second placeer
if (Q.pres_results.length > 1) {
    Q.pres_second_party = Q.pres_results[1].party;
    Q.pres_second_candidate = Q.pres_results[1].candidate;
    Q.pres_second_votes = Q.pres_results[1].votes;
} else {
    Q.pres_second_party = '';
    Q.pres_second_candidate = '';
    Q.pres_second_votes = 0;
}

Q.old_president = Q.president;
Q.old_president_party = Q.president_party;

//update president
if (winnerParty && candidateData[winnerParty]) {
    Q.president = Q.pres_winner_candidate;
    Q.president_party = winnerParty.toUpperCase();
} else {
    Q.president = Q.old_president;
    Q.president_party = Q.old_president_party;
}

//party colors for display
var partyColors = {
    'UNP': '#006400', 'SLFP': '#00008B', 'TULF': '#F7D917', 'CWC': '#E52B50',
    'ACTC': '#FDCC01', 'CPSL': '#65000B', 'JVP': '#FF2226', 'LSSP': '#A91101',
    'NSSP': '#FF8080', 'SLPP': '#800020', 'SLMP': '#663399', 'SLMC': '#2DCF3E',
    'NPP': '#9F005D', 'MEP': '#FF3915', 'EROS': '#FF2400', 'DPLF': '#CE2029', 'PFLT': '#DC143C', 'UCPF': '#9C1600'
};

Q.pres_winner_color = (winnerParty && partyColors[winnerParty.toUpperCase()]) ? partyColors[winnerParty.toUpperCase()] : '#888888';

//check for player victory
Q.slfp_pres_victory = (winnerParty && winnerParty === 'slfp') ? 1 : 0;

//check for cohabitation possibilities
if (Q.prime_minister_party && Q.prime_minister_party !== Q.president_party) {
    Q.cohabitation = 1;
} else {
    Q.cohabitation = 0;
}

!}
